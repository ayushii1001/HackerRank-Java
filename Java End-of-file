import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String n=sc.nextLine();
        int i=1;
        do{
            System.out.println(i+" "+n);
            if (sc.hasNextLine()){
                n=sc.nextLine();
            }
            else{
                n=null;
            }
            i++;
        }while(n!=null);
    }
}
